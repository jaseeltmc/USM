<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Bootstrap demo</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
</head>

<body>

    <div class="d-grid gap-2 d-flex justify-content-end mt-5 me-5">
        <button class="btn btn-outline-secondary " type="button"><a href="/logout" style="text-decoration: none;">Log Out</a></button>
      </div>
<div class="container">
  <div class="image d-grid gap-2 d-flex justify-content-center mt-5">
    <img src="https://www.freepnglogos.com/uploads/adidas-logo-png-black-0.png" alt="">
  </div>
</div>
  <section>
    <div class="container mt-5">
      <div class="row gx-4 gy-4">
       
        <% for(var i=0; i<product.length; i++) { %>
           
            
            <div class="col-md-4">
                <div class="card" style="width: 18rem;">
                  <img
                    src="<%= product[i].Link %>"
                    class="card-img-top" alt="...">
                  <div class="card-body">
                    <h5 class="card-title"><%= product[i].title %></h5>
                    <p class="card-text"><%= product[i].Description %></p>
                    <a href="#" class="btn btn-primary">Add to Cart</a>
                  </div>
                </div>
              </div>

         <% } %>
       
      </div>
    </div>

  </section>



  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
    crossorigin="anonymous"></script>
</body>

</html>